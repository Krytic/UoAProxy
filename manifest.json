{

  "manifest_version": 2,
  "name": "UoAProxy",
  "version": "1.0",

  "description": "Reroutes journal websites through the UoA Proxy",

  "icons": {
    "48": "icons/border-48.png"
  },

  "content_scripts": [
    {
      "matches": ["*://journals.aps.org/*"],
      "js": ["redirect.js"]
    }
  ]

}
